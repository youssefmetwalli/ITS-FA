{
    "version": 2,
    "builds": [
      { "src": "api/chat.py", "use": "@vercel/python" },
      { "src": "api/app.py", "use": "@vercel/python" }
    ],
    "routes": [
      { "src": "/chat_api", "dest": "api/chat.py" },
      { "src": "/chat", "dest": "api/chat.py" },
      { "src": "/(.*)", "dest": "api/app.py" }
    ]
  }
  